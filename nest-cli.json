{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/learning/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/learning/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/learning",
  "projects": {
    "learning": {
      "type": "application",
      "root": "apps/learning",
      "entryFile": "main",
      "sourceRoot": "apps/learning/src",
      "compilerOptions": {
        "tsConfigPath": "apps/learning/tsconfig.app.json"
      }
    },
    "computerix": {
      "type": "application",
      "root": "apps/computerix",
      "entryFile": "main",
      "sourceRoot": "apps/computerix/src",
      "compilerOptions": {
        "tsConfigPath": "apps/computerix/tsconfig.app.json"
      }
    }
  }
}